<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NVVM_InternetMarket.Resources"
                    xmlns:vm="clr-namespace:NVVM_InternetMarket.ViewModel"
                    xmlns:v="clr-namespace:NVVM_InternetMarket.View">
    <DataTemplate DataType="{x:Type vm:CategoriesVM}">
        <v:CategoriesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ItemsListVM}">
        <v:ItemsListView/>
    </DataTemplate>
    <Style TargetType="Label">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Foreground" Value="{StaticResource ControlBackgroundOver}"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
    </Style>
</ResourceDictionary>